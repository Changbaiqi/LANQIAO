package lanqiao;

import java.util.Scanner;

public class 最小权值 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		long dp[] = new long[2022];
		dfs(dp,2021);
	}
	
	
	public static void dfs(long dp[],int root) {
		if(root==0)
			return;
		
		for(int L= 0; L <=root-1 ; ++L) {
			int R = 2021-L;
			dfs(dp,R);
			dfs(dp,L);
		}
	}

}
