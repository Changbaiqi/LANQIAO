 */
public class °Ú»¨ {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		
		int n = sc.nextInt();
		int m = sc.nextInt();
		
		int data[] = new int[n+1];
		for(int i=1 ; i <=n ; ++i) {
			data[i] = sc.nextInt();
		}
		
		
		int dp[][] = new int[n+1][m+1];
		dp[0][0] = 1;
		
		for(int y =1 ;y<=n ; ++y ) {
	
			for(int x= 1; x<=m;++x) {
				dp[y][x] +=dp[y-1][x-x]
			}
		}
	}

}
