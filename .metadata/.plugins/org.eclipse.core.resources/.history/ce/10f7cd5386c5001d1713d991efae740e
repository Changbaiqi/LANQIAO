package lanqiao;

import java.util.Scanner;

public class BÕÒÉ½ÆÂ {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		Scanner sc = new Scanner(System.in);
		
		int n = sc.nextInt();
		
		int data[] = new int[n];
		int index[] = new int[n];
		for(int i = 0; i< n ; ++i) {
			data[i] = sc.nextInt();
		}
		
		for(int i = 0 ; i< n ; ++i) {
			int ans =0,sw=0;
			for(int x = i+1; x < n ; ++x ) {
				if(data[i]>=data[x]) {
					++ans;
				}
				if(data[i]==data[x]) {
					sw=1;
					index[i]= ans;
				}
				if(data[i]<data[x])
					break;
			}
			
		}
		
		
		
	}

}
