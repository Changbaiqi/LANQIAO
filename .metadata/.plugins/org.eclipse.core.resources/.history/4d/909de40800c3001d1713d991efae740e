package lanqiao;

public class ≈≈–Ú {

	public static void main(String[] args) {
	// TODO Auto-generated method stub
		
		char data[] = "abcdefghijklmno".toCharArray();
		int sum = 0;
			for(int i= 0; i < data.length ; ++i) {
				for(int x=i+1 ; x < data.length ; ++x) {
					if(data[i]< data[x]) {
						char res = data[i];
						data[i] = data[x];
						data[x] = res;
						++sum;
						if(sum==100) {
							System.out.println(data.toString());
						}
							
					}
				}
			}
			
	}

}
