package lanqiao;

import java.util.Scanner;

public class 金明的预算方案 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		Scanner sc = new Scanner(System.in);
		
		int n = sc.nextInt();
		int m = sc.nextInt();
		
		int v[] = new int[m+1];
		int w[] = new int[m+1];
		int k[] = new int[m+1];
		
		
		for(int i=1; i <= m ; ++i) {
			v[i] = sc.nextInt();
			w[i] = sc.nextInt();
		}
		
	}

}
