package lanqiao;

import java.util.ArrayList;
import java.util.LinkedList;
import java.util.Queue;
import java.util.Scanner;

public class Kevin喜欢零简单版本 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		Scanner sc = new Scanner(System.in);
		//一共多少组测试条例
		int T = sc.nextInt();
		
		for(int i= 1; i <=T ; ++i) {
			int n = sc.nextInt(); //序列长度
			int k = sc.nextInt(); //后导零数量
			
			
			Num data[] = new Num[n+1];
			for(int x =1 ; x <=n; ++x) {
				int a= sc.nextInt();
				data[x] = new Num(a,modAns(a,5),modAns(a,2));
			}
 			System.out.println(slove(data,k));
			
			
		}
		
		
		
		
	}
	
	//余数数量
	public static int modAns(int x,int mod) {
		int ans = 0;
		while(x%mod==0) {
			x/=mod;
			++ans;
		}
		return ans;
	}
	
	public static long slove(Num data[],int k) {
		int start =1;
		int end =1;
		int five =0;
		int two =0;
		long ans =0;
		while(start<data.length) {
			if(start!=end) {
				five -= data[end].five;
				two -= data[end].five;
			}
			while(end<data.length) {
				
				five += data[end].five;
				two += data[end].five;
				
				//比较是否符合条件
				int minAns = Math.min(five, two);
				if(minAns ==k)
					++ans;
				
			
				if(minAns<=k)
					++end;
			}
			++start;
			if(end<start)
				++end;
		}
		return ans;
	}
	
	public static class Num{
		int num;
		int five;
		int two;
		public Num(int num, int five, int two) {
			super();
			this.num = num;
			this.five = five;
			this.two = two;
		}
		
	}
}

