	public static void solve(String move) {
		char map[][] = new char[210][210];
		
		int x=105,y=105;
		int i = 0;
		int minX=210,minY =210;
		int maxX =0,maxY = 0;
		do {
			map[y][x] = '#';
			minX = Math.min(minX, x);
			if(move.charAt(i)=='U')
				y-=1;
			if(move.charAt(i)=='D')
				y+=1;
			if(move.charAt(i)=='L')
				x-=1;
			if(move.charAt(i)=='R')
				x+=1;
		}while(i<move.length());
		
	}

