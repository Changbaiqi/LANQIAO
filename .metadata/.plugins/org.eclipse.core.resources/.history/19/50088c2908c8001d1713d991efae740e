	public static boolean check(int cx,int cy,int r,int pointx,int pointy) {
		int rr = (int) (Math.pow((cx-pointx),2)+Math.pow((cy-pointy), 2));
		if(rr<=r*r)
			return true;
		
		return false;
	}

