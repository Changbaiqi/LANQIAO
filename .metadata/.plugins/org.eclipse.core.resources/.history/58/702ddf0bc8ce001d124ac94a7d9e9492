package lanqiao;

import java.util.Arrays;
import java.util.Scanner;

public class ¿∂«≈±≠2022≥ı»¸ªÿ“‰√‘π¨ {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		String move = sc.next();
		solve(move);
	}
	
	
	public static void solve(String move) {
		char map[][] = new char[210][210];
		
		int x=105,y=105;
		int i = 0;
		int minX=210,minY =210;
		int maxX =0,maxY = 0;
		map[105][105] ='#';
		do {
			map[y][x] = '#';
			minX = Math.min(minX, x);
			minY = Math.min(minY, y);
			maxY = Math.max(maxY, y);
			maxX = Math.max(maxX, x);
			
			if(move.charAt(i)=='U')
				y-=1;
			if(move.charAt(i)=='D')
				y+=1;
			if(move.charAt(i)=='L')
				x-=1;
			if(move.charAt(i)=='R')
				x+=1;
			++i;
		}while(i<move.length());
		
		
		char resMap[][] = new char[maxY-minY][maxX-minX];
		
		for(int e =0 ; e < resMap.length ; ++e) {
			resMap[e] = Arrays.copyOfRange(map[e+minY], minX, maxX);
		}
		prMap(resMap);
	}
	
	
	public static void prMap(char map[][]) {
		for(int y =0 ; y < map.length ; ++y) {
			for(int x= 0 ; x < map[y].length ; ++x) {
				System.out.print(map[y][x]+" ");
			}
			System.out.println();
		}
	}

}
