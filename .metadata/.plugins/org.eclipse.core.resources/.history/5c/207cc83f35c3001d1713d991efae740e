package lanqiao;

import java.util.Scanner;

public class 试试看贪心 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		int k = sc.nextInt();
		
		//早上
		int light[] = new int[n];
		
		//晚上
		int right[] = new int[n];
		
		for(int i = 0 ; i < light.length ; ++i) {
			light[i] = sc.nextInt();
		}
		
		for(int i = 0 ; i < right.length ; ++i) {
			right[i] = sc.nextInt();
		}
		
		
	}
	

}

class Co{
	int light;
	int right;
	boolean choose=false;
	public Co(int light, int right) {
		super();
		this.light = light;
		this.right = right;
	}
	
}
