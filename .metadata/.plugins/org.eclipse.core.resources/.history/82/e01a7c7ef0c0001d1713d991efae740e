package lanqiao;

import java.util.Scanner;

public class Í¡²©¶û {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		Scanner sc = new Scanner(System.in);
		
		int n = sc.nextInt();
		int S = sc.nextInt();
		int L = sc.nextInt();
		
		int tree[] = new int[n];
		
		for(int i= 0; i < n ; ++i) {
			tree[i] = sc.nextInt();
		}
		
		int sz[] = new int[n];
		for(int i = 0; i < n ; ++i) {
			sz[i] = sc.nextInt();
		}
		
		int i = 0;
		while(true) {
			int sum = 0;
			for(int x = 0 ; x < n ;++x) {
				if(tree[x]>=L)
					sum+=tree[x];
			}
			if(sum>=S)
				break;
		}
		
		System.out.println(i);
		
		
	}

}
