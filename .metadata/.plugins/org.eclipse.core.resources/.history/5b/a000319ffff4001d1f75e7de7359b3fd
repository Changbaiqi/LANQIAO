package lanqiao;

import java.util.ArrayList;
import java.util.Scanner;

public class 金明的预算方案 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		Scanner sc = new Scanner(System.in);
		
		int n = sc.nextInt();
		int m = sc.nextInt();
		
		int v[] = new int[m+1];
		int p[] = new int[m+1];
		int q[] = new int[m+1];
		
		ArrayList map[] = new ArrayList[m+1];
		
		for(int i=1; i <= m ; ++i) {
			v[i] = sc.nextInt();
			p[i] = sc.nextInt();
			q[i] = sc.nextInt();
		}
		
		
		
		
	}

}
