package lanqiao;

import java.util.Scanner;

public class 种花背包法 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		
		int n = sc.nextInt(); //种类
		int m = sc.nextInt(); //数量
		
		int dp[] = new int[m+1];
		for(int y = 1; y<=n ; ++y ) {
			for(int x=m; x>=1;--x) {
				dp[x] += dp[];
			}
		}
	}

}
