package lanqiao;

import java.util.ArrayList;
import java.util.HashMap;
import java.util.LinkedList;
import java.util.Queue;
import java.util.Scanner;

public class BFS_ {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		
		while(sc.hasNext()) {
			int n = sc.nextInt();
			int m = sc.nextInt();
			int k = sc.nextInt();
			int s = sc.nextInt();
			
			int kk[] = new int[k];
			for(int i=0 ; i < k; ++i) {
				kk[i] = sc.nextInt();
			}
			
			//存的图
			HashMap<Integer, Pointt>
			int mm[][] = new int[m][2];
			for(int i= 0 ; i < m ; ++i) {
				mm[i][0] = sc.nextInt();
				mm[i][1] = sc.nextInt();
			}
			
			
		}
		
	}
	
	
	public static int bfs(int s,int kk[],int mm[][]) {
		//最大时间长度
		int maxT = kk.length;
		
		Queue<int[]> data = new LinkedList<int[]>();
		data.offer(new int[] {s/*第几个石头*/,0/*时间*/});
		
		while(!data.isEmpty()) {
			
		}
		
	}

}

class Pointt{
	int point;
	ArrayList<Integer> next;
	public Pointt(int point, ArrayList<Integer> next) {
		super();
		this.point = point;
		this.next = next;
	}
	public int getPoint() {
		return point;
	}
	public void setPoint(int point) {
		this.point = point;
	}
	public ArrayList<Integer> getNext() {
		return next;
	}
	public void setNext(ArrayList<Integer> next) {
		this.next = next;
	}
	@Override
	public int hashCode() {
		// TODO Auto-generated method stub
		return point;
	}
	@Override
	public boolean equals(Object obj) {
		// TODO Auto-generated method stub
		Pointt p = (Pointt)obj;
		return (this.point == p.getPoint());
	}
	
	
}
