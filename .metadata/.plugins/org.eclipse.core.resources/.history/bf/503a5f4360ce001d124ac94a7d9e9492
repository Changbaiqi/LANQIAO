	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		String str = sc.next();
		
		int data[] = new int['z'+1];
		
		for(int i = 0;i <str.length() ; ++i) {
			++data[str.charAt(i)];
