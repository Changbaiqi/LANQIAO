package lanqiao;

import java.util.Scanner;

public class ¿€º”Œ Ã‚ {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		
		while(sc.hasNext()) {
			String str = sc.next();
			char cc[] = str.toCharArray();
			
			int w=0;
			int l=0;
			for(int i = 0 ; i < cc.length ;++i) {
				if(cc[i]=='W')
					++w;
				if(cc[i]=='l')
					++l;
				
			}
			
		}
		
		
	}

}
