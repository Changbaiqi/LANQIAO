		System.out.println(slove(3));
	}
	
	
	public static boolean slove(int num) {
		if(num==1)
			return false;
		
		for(int i=2 ;i<=Math.sqrt(num);++i) {
			if(num%10==0)
				return false;
		}
		
		
		int res=0;
		while(num!=0) {
			res= num%10;
			num/=10;
			if(!map[res])
				return false;
		}
		
		
		
		return true;
	}

}
