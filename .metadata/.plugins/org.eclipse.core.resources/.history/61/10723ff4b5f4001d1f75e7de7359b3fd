public class Ïß¶Î {
	
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		Scanner sc = new Scanner(System.in);
		
		
		int n = sc.nextInt();
		int L[] = new int[n+1];
		int R[] = new int[n+1];
		
		for(int i= 1 ; i <=n ; ++i) {
			L[i] = sc.nextInt();
			R[i] = sc.nextInt();
		}
		
		int dp[][] = new int[n+1][2];
		dp[0][0] = 1;
		dp[0][1] =1;
		
		System.out.println(Math.min(dp[n][0],dp[n][1]));
		
	}

}
