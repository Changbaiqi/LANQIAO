	public static boolean slove(int num) {
		if(num==1)
			return false;
		
		int numRes = num;
		int res=0;
		while(numRes!=0) {
			res= numRes%10;
			numRes/=10;
			if(!map[res])
				return false;
		}
		
		
		
		
		
		for(int i=2 ;i<=Math.sqrt(num);++i) {
			if(num%i==0)
				return false;
		}
		
		
		
		
		
		
		return true;
	}

}
