	public static int dfs(boolean map[][],int x,int y,int ans,int now) {
		if(ans==now) {
			return 1;
		}
		int sum =0;
		
		int next[][]= {
				{x,y-1},
				{x,y+1},
				{x-1,y},
				{x+1,y}
		};
		dfs(map,x,y,ans,now);
		
		return sum;
	}

