package lanqiao;

import java.util.Scanner;

public class 最长回文字 {

	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		 Scanner sc = new Scanner(System.in);
		 
		 int n = sc.nextInt();
		 
		 String A= sc.nextLine();
		 String B= sc.nextLine();
		 
		 int Ap[] = null,Bp[] = null;
		 String Am = Manacher(A.toCharArray(), Ap);
		 String Bm = Manacher(B.toCharArray(), Bp);

	}
	
	//Manacher算法
	public static String Manacher(char str[],int p[]) {
		String resStr = "$#";
		
		for(int i = 0 ; i < str.length ; ++i) {
			resStr+=str[i];
			resStr+="#";
		}
		resStr+="^";
		
		p = new int[resStr.length()];
		char resChar[] = resStr.toCharArray();
		for(int i = 1 ; i< resChar.length ; ++i) {
			int x=0;
			while(resChar[i-x-1]==resChar[i+x+1]) {++x;}
			p[i] = x;
		}
		
		return resStr;
	}

}
