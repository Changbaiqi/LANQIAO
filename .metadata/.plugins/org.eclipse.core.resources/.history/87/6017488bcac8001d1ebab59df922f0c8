	public static void dfs(boolean map [][],boolean state,int x,int y,int ans) {
		
		
		int next[][]= {
				{x,y-1},
				{x,y+1},
				{x-1,y},
				{x+1,y}
		};
		
		for(int i = 0 ; i < next.length ; ++i) {
			if(next[i][0]<0 || next[i][1]<0 || next[i][0]>=map[0].length || next[i][1]>map.length)
				continue;
			
			
		}
		
	}

