package lanqiao;

import java.util.Scanner;

public class 小明的背包1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		
		int N = sc.nextInt();
		int V = sc.nextInt();
		
		int w[] = new int[N+1];
		int v[] = new int[N+1];
		
		int dp[][] = new int[N+1][V+1];
		for(int i =0 ; i < N ; ++i) {
			w[i] = sc.nextInt();
			v[i] = sc.nextInt();
		}
		
		
		
	}

}
